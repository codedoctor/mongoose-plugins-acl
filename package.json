{
  "author": "Martin Wawrusch <martin@wawrusch.com> (https://www.linkedin.com/in/martinwawrusch)",
  "name": "mongoose-plugins-acl",
  "description": "Basic ACL (access control list) for mongoose.",
  "version": "1.0.1",
  "main": "lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/codedoctor/mongoose-plugins-acl.git"
  },
  "bin": {},
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "keywords": [
    "errors"
  ],
  "scripts": {
    "prepublish": "coffee -c -o lib src",
    "watch": "coffee -c -w  -o lib src",
    "test": "coffee -c -o lib src;NODE_ENV=test node_modules/.bin/mocha --timeout 10000 -R spec test/*.coffee"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "hoek": "2.14.0",
    "underscore": "1.8.3"
